{
  "name": "AminoSee",
  "version": "1.0.1",
  "description": "DNA viewer. A terminal command in node renders a unique colour view of RNA or DNA stored in text files, output to PNG graphics file, then launches an WebGL browser that projects the image onto a 3D Hilbert curve for immersive viewing, using THREEjs.",
  "main": "index.js",
  "dependencies": {
    "ascii-histogram": "^1.2.1",
    "bmp-js": "^0.1.0",
    "bytes": "^3.0.0",
    "comlink-fetch": "^0.1.2",
    "electron-packager": "^12.2.0",
    "event-stream": "^4.0.1",
    "hammerjs": "^2.0.8",
    "jimp": "^0.5.6",
    "jquery": "^3.3.1",
    "keypress": "^0.2.1",
    "localforage": "^1.7.3",
    "minimist": "^1.2.0",
    "node-fetch": "^2.2.1",
    "node-http-server": "^8.1.2",
    "opn": "^5.4.0",
    "parse-apache-directory-index": "^1.2.1",
    "pngjs": "^3.3.3",
    "request": "^2.88.0",
    "save": "^2.3.2",
    "stream-file": "^0.2.3",
    "three": "^0.98.0",
    "uniq": "^1.0.1",
    "vows": "^0.8.2"
  },
  "devDependencies": {},
  "scripts": {
    "test": "vows --spec --isolate",
    "start": "aminosee"
  },
  "keywords": [
