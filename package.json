{
  "name": "aminosee",
  "version": "1.17.1",
  "owner": "Thomas Bieder Atkinson",
  "description": "Allows genomics researchers to convert any file containing ASCII blocks of DNA (Fasta, GBK, .txt) into an image. A unique visualisation of DNA / RNA residing in text files, AminoSee is a way to render arbitrarily large files - due to support for streamed processing - into a static size PNG image using a pseudo-Hilbert infinite space filling curve written in the 18th century! Computation is done locally, and the files do not leave your machine. A back-end terminal daemon cli command that can be scripted is combined with a front-end GUI in Electron, AminoSee features asynchronous streaming processing enabling arbitrary size files to be processed. It has been tested with files in excess of 4 GB and does not need the whole file in memory at any time. In a diectory with DNA files try running aminosee *. Alternatively use the GUI to Drag and drop files to render a unique colour view of RNA or DNA stored in text files, output to PNG graphics file, then launches an WebGL browser that projects the image onto a 3D Hilbert curve for immersive viewing, using THREEjs. Command line options alow one to filter by peptide.",
  "bin": {
    "aminosee": "./bin/aminosee.js"
  },
  "main_bundle": "bundle.js",
  "main": "main.js",
  "build": {
    "productName": "aminosee",
    "appId": "nz.funk.aminosee",
    "copyright": "Copyright Â© 2018-2019 Tom Atkinson",
    "icon": "public/favicon.png",
    "directories": [
      "public/**",
      "dist/root/**"
    ],
    "mac": {
      "category": "your.app.category.type"
    }
  },
  "dependencies": {
    "ascii-histogram": "^1.2.1",
    "async-kit": "^2.2.4",
    "body-parser": "^1.19.0",
    "browserify": "^16.2.3",
    "bytes": "^3.1.0",
    "chalk": "^2.4.2",
    "cross-spawn": "^6.0.5",
    "event-stream": "^4.0.1",
    "express": "^4.17.1",
    "fs-extra": "^8.0.1",
    "get-cursor-position": "^1.0.5",
    "golden-layout": "^1.5.9",
    "hammerjs": "^2.0.8",
    "highland": "^2.13.4",
    "hilbert-2d": "0.0.1",
    "http-server": "^0.11.1",
    "humanize-duration": "^3.18.0",
    "internal-ip": "^4.3.0",
    "jimp": "^0.6.4",
    "keypress": "^0.2.1",
    "local-web-server": "^3.0.3",
    "minimist": "^1.2.0",
    "nexe": "^3.3.1",
    "node-fetch": "^2.6.0",
    "node-gyp": "^5.0.0",
    "npm-merge-driver": "^2.3.5",
    "open": "^6.3.0",
    "parse-apache-directory-index": "^1.2.1",
    "png-js": "^0.1.1",
    "png-stream": "^1.0.5",
    "png.js": "^0.2.1",
    "pngjs": "^3.4.0",
    "preferences": "^1.0.2",
    "remark-preset-lint-node": "^1.6.1",
    "remove": "^0.1.5",
    "request": "^2.88.0",
    "save": "^2.4.0",
    "serve": "^11.0.2",
    "sliced": "^1.0.1",
    "spdy": "^4.0.0",
    "stream-file": "^0.2.4",
    "terminal-kit": "^1.28.1",
    "three": "^0.105.2",
    "through": "^2.3.8",
    "uniq": "^1.0.1",
    "vows": "^0.8.2",
    "xmlhttprequest": "^1.8.0",
    "xterm": "^3.14.2"
  },
  "devDependencies": {
    "electron": "^5.0.4",
    "electron-rebuild": "^1.8.5",
    "genversion": "^2.1.1"
  },
  "scripts": {
    "credits": "./build-credits.sh",
    "crash": "node --inspect 27MB_TestPattern.txt --peptide=Ochre 3MB_TestPattern.txt 50KB_TestPattern.txt batch-peptides.sh batch-process.sh Gorilla_gorGor4_chr2B.fa",
    "test": "echo 'requires cli program time' ; time dna/run-test.sh ; vows --spec --isolate",
    "start": "node bin/aminosee",
    "inspect": "node --inspect bin/aminosee.js /Volumes/aminosee-1/dna/Brown_Kiwi_NW_013982187v1.fa   /Volumes/aminosee/dna/Brown_Kiwi_NW_013982187v1.fa  -f -c1000 -vd --debug",
    "demo": "node bin/aminosee demo",
    "build": "./build-lib.sh ; nice time npm run build-web ; nice time npm run pkg",
    "build-all": "./build-lib.sh ; npm run build-web ; npm run pkg; npm run zip",
    "build-web": "npm run genversion  &amp;  npm run browserify  &amp; npm run minify ; npm run credits",
    "genversion": "genversion aminosee-version.js",
    "browserify": "browserify bin/aminosee.js -o public/bundle.js",
    "minify": "uglify public/bundle.js -o public/bundle.min.js",
    "electron": "electron-packager ./bin/aminosee.js --platform=darwin --arch=x64 --prune --overwrite  --out ./dist/electron",
    "electron-all": "npm run build-all ; electron-packager ./bin/aminosee.js --platform=darwin,win32,linux --arch=x64 --prune --overwrite  --out ./dist/electron",
    "gui": "electron .",
    "guidev": "electron . --devmode --remote-debugging-port 8315 --host-rules='MAP * 127.0.0.1'",
    "pkg": "npm run macos &amp;  nice npm run win32 &amp; nice npm run win64 &amp; nice npm run linux",
    "zip": "zip -8 -r ./dist/AminoSee_macos.zip ./dist/AminoSee_macos  &amp; nice zip -8 -r ./dist/AminoSee_win.zip ./dist/AminoSee_win &amp; nice zip -8 -r ./dist/AminoSee_linux.zip ./dist/AminoSee_linux",
    "runmac": "open ./dist/AminoSee_macos/aminosee-macos",
    "macos": "echo MACOS ; pkg . --targets macos   --output ./dist/AminoSee_macos/aminosee",
    "win32": "echo WIN32 ; pkg . --targets win-x86 --output ./dist/AminoSee_win/aminosee-win32.exe",
    "win64": "echo WIN64 ; pkg . --targets win-x64 --output ./dist/AminoSee_win/aminosee.exe",
    "linux": "echo LINUX ; pkg . --targets linux   --output ./dist/AminoSee_linux/aminosee",
    "_macos": "echo MACOS ; pkg . --targets macos   --output ./dist/AminoSee_macos/aminosee-macos   ; zip -8 -r ./dist/AminoSee_macos.zip ./dist/AminoSee_macos",
    "_win32": "echo WIN32 ; pkg . --targets win-x86 --output ./dist/AminoSee_win/aminosee-win32.exe ;",
    "_win64": "echo WIN64 ; pkg . --targets win-x64 --output ./dist/AminoSee_win/aminosee-win64.exe ; zip -8 -r ./dist/AminoSee_win.zip ./dist/AminoSee_win",
    "_linux": "echo LINUX ; pkg . --targets linux   --output ./dist/AminoSee_linux/aminosee-linux   ; zip -8 -r ./dist/AminoSee_linux.zip ./dist/AminoSee_linux"
  },
  "keywords": [
    "dna",
    "genomics",
    "visualisation",
    "renderer",
    "genetics",
    "bioinformatics",
    "bionode",
    "amino acids",
    "peptides",
    "codons"
  ],
  "author": "Tom Atkinson <tom@funk.co.nz>",
  "website": "http://aminosee.funk.nz",
  "license": "GPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "git://github.com/tomachinz/AminoSee.git"
  },
  "pkg": {
    "targets": [
      "linux",
      "macos",
      "win"
    ],
    "scripts": [
      "dist/root/pos.node"
    ],
    "assets": [
      "bin/pos.node",
      "node_modules/opn/**",
      "node_modules/electron/dists/**",
      "node_modules/terminal-kit/**",
      "node_modules/get-cursor-position/**",
      "bin/**",
      "index.html",
      "index.php",
      "aminosee-cli.js",
      "aminosee-data.js",
      "aminosee-settings.js",
      "aminosee-server.js",
      "aminosee-version.js",
      "public/**",
      "package.json",
      "dna/megabase.fa"
    ]
  }
}
